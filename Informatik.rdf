    <ns2:hasCourse rdf:resource="http://tuc/course#Informatik"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#bd216178-c42e-11ee-ae72-d43d7e41305b">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns3:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">561030</ns3:hasModuleNumber>
    <ns3:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Multicore-Programmierung</ns3:hasName>
    <ns3:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">tionsziele Die Inhalte der Lehrveranstaltungen umfassen eine Einführung in die Archi-tektur von Multicore-Prozessoren, Programmiermodelle zur Multicore-Pro-grammierung und die Programmierung mit Threads. Zur Thread-Program-mierung werden verschiedene Sprach- und Bibliothekansätze vorgestellt.</ns3:hasContent>
    <ns3:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns3:hasCreditPoints>
