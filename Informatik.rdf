  <rdf:Description rdf:about="http://tuc.web.engineering/module#bd21fdd1-c42e-11ee-8097-d43d7e41305b">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns3:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">578050</ns3:hasModuleNumber>
    <ns3:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mediencodierung</ns3:hasName>
    <ns3:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">tionsziele Es werden zentrale Aspekte der Informationstheorie, Codierung medialerDaten und Kompressionsalgorithmen besprochen. Ferner wird diskutiert,wie diese bei der Codierung einzelner Medientypen Verwendung finden.</ns3:hasContent>
