  <rdf:Description rdf:about="http://tuc.web.engineering/module#bd2a94a2-c42e-11ee-af71-d43d7e41305b">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns3:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SPZ_Pol_3</ns3:hasModuleNumber>
