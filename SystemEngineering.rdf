  <rdf:Description rdf:about="http://tuc.web.engineering/module#46154f33-c42e-11ee-9cb5-d43d7e41305b">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1.1.10</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Supply Chain Management</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Qualifikationsziele Das Supply Chain Management befasst sich mit der ganzheitlichenBetrachtung von miteinander vernetzten Unternehmen. Hier spielen nebenManagement-Ansätzen quantitative Methoden zur Generierung vonNetzwerken und die Einbeziehung von Soft Facts eine wesentliche Rolle.</ns2:hasContent>
    <ns2:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns2:hasCreditPoints>
    <ns2:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">150</ns2:hasWorkLoad>
