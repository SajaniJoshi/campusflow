    <module:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Einführung in Data Science   </module:hasName>
    <module:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Umgang mit Daten (Erhebung, Vorverarbeitung, Visualisierung)   Statistische Lernverfahren (Regression, neuronale Netze, Resampling-Verfahren, Modellauswahl)   Klassifikation (baum- und kernbasierte Methode)   Bayessche Methoden, Informationstheorie   Anwendungen: Sprach- und Bildverarbeitung</module:hasContent>
    <module:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</module:hasCreditPoints>
  </rdf:Description>
