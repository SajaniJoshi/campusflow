    <module:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Operatives Produktionsmanagement  </module:hasName>
    <module:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Advanced Planning Systeme   Prognoseverfahren   Modelle und Verfahren der Produktionsprogrammplanung   Bestandsmanagement und Losgrößenplanung   Ablaufplanung   Ausgewählte Modelle und Methoden in der Produktionssteuerung</module:hasContent>
    <module:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</module:hasCreditPoints>
