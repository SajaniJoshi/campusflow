    <module:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">621020 </module:hasModuleNumber>
    <module:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Betriebliche Umweltökonomie und Nachhaltigkeitsmanagement  </module:hasName>
    <module:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Essentielle Begrifflichkeiten des Nachhaltigkeitsmanagements sowie konzeptionelle und strategische Grundlagen einer nachhaltigen Unterne h-mensführung   Beiträge der primären Akteure im Feld der Nachhaltigkeit   Ganzheitliche Betrachtungen, z.B. Wertschöpfungsketten und Lebenszyk-lusansätze   Instrumente einer betrieblichen Umwelt ökonomie und nachhaltigen Unter-nehmensführung in verschiedenen unternehmerischen Funktionsberei-chen   Praxisangewendete Methoden der empirischen Sozialforschung und de-ren Reflexion</module:hasContent>
    <module:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</module:hasCreditPoints>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#_Datenbanken_und_Web_-Techniken__">
