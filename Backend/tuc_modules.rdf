    <module:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Datenbanken und Web -Techniken  </module:hasName>
    <module:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Basistechniken der Internetprogrammierung zum Zugriff auf Datenban-ken,  Datenbankabstraktionsschichten, semistrukturierte Daten, Web -Ser-vices</module:hasContent>
    <module:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</module:hasCreditPoints>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#_Arbeitsrecht__">
