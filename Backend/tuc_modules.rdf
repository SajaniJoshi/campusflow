    <module:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">261034-301</module:hasModuleNumber>
    <module:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Asset Management  </module:hasName>
    <module:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Grundlagen des Asset Management   Assetklassen   Ausgewählte Strategien des Asset Management   Anbieter und Nachfrager von Asset- Management-Leistungen</module:hasContent>
