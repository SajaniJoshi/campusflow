    <module:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">M-Ma-I12 </module:hasModuleNumber>
    <module:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Parallele Programmierung  </module:hasName>
    <module:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Die Inhalte der Vorlesung umfassen eine Übersicht über die Architektur von Parallelrechnern mit einem besonderen Schwerpunkt auf Verbindungsnetzwerken, parallelen Leistungsmaßen und Laufzeitanalyse, Message-Passing-Programmierung und Kommunikationsmuster.</module:hasContent>
    <module:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</module:hasCreditPoints>
