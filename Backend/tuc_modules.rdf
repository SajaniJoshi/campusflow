    <module:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Recht des Vertriebs, Vertriebssteuerung   Produkt-, Preispolitik, Qualitätsmanagement   Vertriebskalkulation und-controlling   Neuromarketing</module:hasContent>
    <module:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</module:hasCreditPoints>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#_Jahresabschluss__">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <module:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">261040-100</module:hasModuleNumber>
