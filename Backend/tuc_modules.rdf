    <module:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Neurophysik  </module:hasName>
    <module:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Biophysikalische Grundlagen neuronaler Verarbeitung   Schaltkreismodelle neuronaler Verarbeitung   Signalübertragung in neuronalen Systemen   Neuronale Kodierung   Neuronale Netzwerke   Synaptische Übertragung   Lernprozesse</module:hasContent>
    <module:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</module:hasCreditPoints>
  </rdf:Description>
