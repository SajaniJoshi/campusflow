  <rdf:Description rdf:about="http://tuc.web.engineering/module#_Betriebssysteme_für_verteilte_Systeme__">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <module:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">565010 </module:hasModuleNumber>
    <module:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Betriebssysteme für verteilte Systeme  </module:hasName>
    <module:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Spezielle Probleme von Betriebssystemen in verteilten Systemen; Algorith-men für Basisprobleme (Mutex, Terminierung, Auswahl, etc.); Uhren in ver-teilten Systemen; Gruppenkommunikation; Zuordnung und Lastbalancie-rung; Namen; verteilte Betriebssysteme; verteilte Transaktionen; Fallbei-spiele (z.B. Mach, Plan9/Inferno, Amoeba)</module:hasContent>
    <module:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</module:hasCreditPoints>
    <ns1:hasUniversity rdf:resource="http://across/university#TUC"/>
