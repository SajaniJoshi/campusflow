  <rdf:Description rdf:about="http://tuc.web.engineering/module#_Virtuelle_Realität__">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <module:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">571250 </module:hasModuleNumber>
