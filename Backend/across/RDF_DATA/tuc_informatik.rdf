    <module:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string"> Theoretische Informatik II  </module:hasName>
    <module:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Automaten, Grammatiken, Chomsky Hierarchie, Turing Maschinen, Nicht -Entscheidbarkeit, NP -Vollständigkeit</module:hasContent>
    <module:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</module:hasCreditPoints>
    <ns1:hasUniversity rdf:resource="http://across/university#TUC"/>
