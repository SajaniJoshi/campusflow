    <university:hasUniversity rdf:resource="http://across/university#TUC"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#ISFME00202">
    <university:hasUniversity rdf:resource="http://across/university#BU"/>
