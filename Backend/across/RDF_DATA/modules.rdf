  <rdf:Description rdf:about="http://tuc.web.engineering/module#ISFCS00056S">
    <module:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Digital Circuit Engineering </module:hasName>
    <module:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IS-FCS-00056S</module:hasModuleNumber>
