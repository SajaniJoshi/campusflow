    <ns2:hasUniversity rdf:resource="http://across/university#TUC"/>
    <ns3:hasCourse rdf:resource="http://tuc/course#Informatik"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#f6ae028c-c5e2-11ee-b84c-d43d7e41305b">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">SPZ_Span_4</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Spanisch IV (Niveau B1)</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">• Konsolidierung und Erweiterung der Sprachkenntnisse • Vertiefung der Lexik im Bereich Beruf, Studium • Lebenslauf • Landeskundliche/kulturelle Besonderheiten • Grammatische Strukturen: subjuntivo presente, oraciones pasivas, Nebensätze mit indicativo und subjuntivo, indirekte Rede Die Ausbildung orientiert sich an der Sprachkompetenzstufe B1 des Gemein- samen Europäischen Referenzrahmens für Sprachen (GER).</ns1:hasContent>
