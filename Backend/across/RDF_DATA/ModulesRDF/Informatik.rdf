  <rdf:Description rdf:about="http://tuc.web.engineering/module#f6afb03c-c5e2-11ee-b75a-d43d7e41305b">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">642010</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Arbeitsrecht</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Das Modul baut auf zivilrechtlichen Grundkenntnissen auf und beinhaltet ver- tiefende und unternehmensspezifische Kenntnisse zum Individualarbeits- recht sowie zum kollektiven Arbeitsrecht.</ns1:hasContent>
