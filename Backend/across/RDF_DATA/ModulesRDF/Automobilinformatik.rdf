    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Datenbanken Grundlagen</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Datenmodelle; Operationen; SQL; Datenmodellierung; Physische Datenorgani- sation; Datenverwaltung; Transaktionsmanager</ns2:hasContent>
