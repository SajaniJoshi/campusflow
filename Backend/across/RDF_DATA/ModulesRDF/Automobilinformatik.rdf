    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns2:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">555030</ns2:hasModuleNumber>
    <ns2:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grundlagen der Technischen Informatik</ns2:hasName>
    <ns2:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Modellierungs- und Spezifikationstechniken für digitale Funktionen; Optimie- rungsverfahren für digitale Schaltungen; Hardwarebeschreibungssprache VHDL, Steu- erwerks- und Datenpfadentwurf</ns2:hasContent>
