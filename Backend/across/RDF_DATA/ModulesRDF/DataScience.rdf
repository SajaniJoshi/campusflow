    <ns3:hasCourse rdf:resource="http://tuc/course#DataScience"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#ad47be69-c5e2-11ee-b3f0-d43d7e41305b">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">M04</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Einführung in die Diskrete Mathematik</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aus zentralen Bereichen der Diskreten Mathematik, wie etwa Kombinatorik, Graphen-, Matroid- und Komplexitätstheorie werden grundlegende Begriffe, Sätze, Beweistechniken und Algorithmen dargestellt.</ns1:hasContent>
