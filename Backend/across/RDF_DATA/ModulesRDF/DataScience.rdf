  <rdf:Description rdf:about="http://tuc.web.engineering/module#ad4a2f37-c5e2-11ee-bf7c-d43d7e41305b">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">M27</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Statistik in Data Science</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">• Explorative Datenanalyse (erkundende Statistik) • Deskriptive Statistik • Large sample theory • Mathematische Statistik • Asymptotische Statistik • Extremwertstatistik • Large deviation theory</ns1:hasContent>
    <ns1:hasLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">English</ns1:hasLanguage>
