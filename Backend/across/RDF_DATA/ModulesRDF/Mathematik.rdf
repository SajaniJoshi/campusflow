    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">241031-004(Version01)</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Modellbildung und Identifikation dynamischer Systeme 1</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">• Modellbegriff • Methoden der Modellbildung • Blackbox- und Whitebox-Modelle • Modellvalidierung • Konkrete Beispiele aus Elektrotechnik, Mechanik, Thermodynamik, Biologie, Chemie</ns1:hasContent>
