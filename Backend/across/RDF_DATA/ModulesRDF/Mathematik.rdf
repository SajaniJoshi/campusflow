  <rdf:Description rdf:about="http://tuc.web.engineering/module#0e11de17-c5e4-11ee-8b51-d43d7e41305b">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">261037-201(Version01)</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Operations Research</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">• Einführung in die mathematische Modellbildung • Grundlagen der Entscheidungstheorie • Lineare und ganzzahlige Optimierung • Produktionsprogrammplanung • Rundreise- und Reihenfolgeplanung • Transportplanung • Zuordnungsprobleme • Netzplantechniken • Simulation und Heuristiken</ns1:hasContent>
    <ns1:hasLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">German</ns1:hasLanguage>
