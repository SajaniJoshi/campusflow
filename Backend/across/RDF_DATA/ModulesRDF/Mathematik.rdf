    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">M-Ma-PS07</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vertiefende Aspekte des Gesundheitsmanagements</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Das Modul beinhaltet Themen des betrieblichen Gesundheitsmanagements in unterschiedlichen Settings und beleuchtet die verschiedenen Stakeholder in diesem Bereich.</ns1:hasContent>
