    <ns3:hasCourse rdf:resource="http://tuc/course#Mathematik"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#0e13167a-c5e4-11ee-bb59-d43d7e41305b">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">261042-200(Version01)</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Betriebliche Umweltökonomie und Nachhaltigkeitsmanagement</ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">• Essentielle Begrifflichkeiten des Nachhaltigkeitsmanagements sowie konzeptionelle und strategische Grundlagen einer nachhaltigen Unternehmensführung • Beiträge der primären Akteure im Feld der Nachhaltigkeit • Ganzheitliche Betrachtungen, z.B. Wertschöpfungsketten und Lebenszyklusansätze • Instrumente einer betrieblichen Umweltökonomie und nachhaltigen Unternehmensführung in verschiedenen unternehmerischen Funktionsbereichen • Praxisangewendete Methoden der empirischen Sozialforschung und deren Reflexion</ns1:hasContent>
    <ns1:hasLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string">German</ns1:hasLanguage>
    <ns1:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ns1:hasCreditPoints>
    <ns1:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">150</ns1:hasWorkLoad>
