    <ns3:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Financial management</ns3:hasName>
    <ns3:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></ns3:hasContent>
