  <rdf:Description rdf:about="http://tuc.web.engineering/module#ab42f33c-d369-11ee-89f6-b03cdc244b9e">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns3:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IS-FME-00255S</ns3:hasModuleNumber>
    <ns3:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Basics of biochemistry and biosensors</ns3:hasName>
    <ns3:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Biosensors-classification, structure, functions. Implementation of transducers in biosensors. Characteristics of selected biosensors. Dry tests for fast medical analysis. Immunosensors. Biopotentials and phenomena at electrode-tissue contact. Trends in development of biosensors.</ns3:hasContent>
    <ns3:hasCreditPoints rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</ns3:hasCreditPoints>
    <ns3:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">84</ns3:hasWorkLoad>
