    <ns1:hasWorkLoad rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">120</ns1:hasWorkLoad>
    <ns3:hasUniversity rdf:resource="http://across/university#TUC"/>
    <ns2:hasCourse rdf:resource="http://tuc/course#TUCSystemEngineering"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#a29ebc08-c2d1-11ee-9880-cc153190a929">
    <rdf:type rdf:resource="http://tuc.web.engineering/module#"/>
    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2.1.10</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Tschechisch III (Niveau B1)  </ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vermittlung grundlegender Sprachkenntnisse und -fertigkeiten, Übersicht über den gesamten Formenbestand der Zielsprache   Vermittlung landeskundlicher Grundkenntnisse, Gebrauch der wichtigsten Wörterbücher und Nachschlagewerke Die Au sbildung orientiert sich an der Sprachkompetenzstufe B1 des Gemeinsamen Europäischen Referenzrahmens für Sprachen (GER).</ns1:hasContent>
