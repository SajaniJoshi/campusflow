    <ns3:hasCourse rdf:resource="http://tuc/course#TUCMathematik"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://tuc.web.engineering/module#4a2ec249-c2cf-11ee-aa5d-cc153190a929">
