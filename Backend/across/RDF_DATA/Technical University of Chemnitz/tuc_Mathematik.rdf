    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">M-Ma-S06</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aufmerksamkeit und Augenbewegungen  </ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Methoden der Aufmerksamkeitsmessung   Modelle von Aufmerksamkeitsprozessen   Methoden der Augenbewegungsmessung   Anwendungen der Augenbewegungsmessung</ns1:hasContent>
