    <ns1:hasModuleNumber rdf:datatype="http://www.w3.org/2001/XMLSchema#string">261040-301</ns1:hasModuleNumber>
    <ns1:hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unternehmensbewertung  </ns1:hasName>
    <ns1:hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Konzeptionelle Grundlagen   Bewertungsanlässe   Vorbereitende Maßnahmen   Bewertungsverfahren   Zwecke der Bewertung von Unternehmen und Unternehmensteilen</ns1:hasContent>
